#Declaración del dataframe con las partículas iniciales
l<-1.5
velocidad<-l/60
n <- 10 #Número de particulas
particulas <- data.frame(x = double(), y=double(), c =double(), r=double(), dx=double(), dy=double(), espesor=double())
xmax <- max(particulas$x)
xmin <- min(particulas$x)
particulas$x <- (particulas$x - xmin) / (xmax - xmin) # ahora son de 0 a 1
ymax <- max(particulas$y)
ymin <- min(particulas$y)
particulas$y <- (particulas$y - ymin) / (ymax - ymin) # las y tambien
